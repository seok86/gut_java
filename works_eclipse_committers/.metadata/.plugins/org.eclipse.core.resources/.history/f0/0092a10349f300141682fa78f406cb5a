package com.bonavision.irsp.ui.views.plan.actions;

import org.eclipse.jface.action.Action;

import com.bonavision.irsp.ui.wizards.plan.NewPlanFolderWizard;
import com.bonavision.ui.wizards.WizardUtil;

/**
 * <pre>
 * 
 * </pre> 	
 * @filename: NewPlanProjectAction.java
 * @copyright: Bonavision, Inc. 2013.
 * @created 2013. 8. 14.
 * @author Yongseok Kim (yongseok.kim@bonavision.net)
 */
public class NewPlanFolderAction extends Action {
	
	public NewPlanFolderAction() {
		super("New Folder", null);
	}
	
	@Override
	public void run() {
		WizardUtil.openNewWizard(NewPlanFolderWizard.ID);
	}
	
}
